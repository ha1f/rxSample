# platform :ios, '9.0'

target 'rxSample' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
	use_frameworks!

  # Pods for rxSample
	pod 'RxSwift',    '~> 2.0'
	pod 'RxCocoa',    '~> 2.0'

  target 'rxSampleTests' do
    inherit! :search_paths
    # Pods for testing
		pod 'RxBlocking', '~> 2.0'
		pod 'RxTests',    '~> 2.0'
  end

  target 'rxSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
